        "idb": "^7.0.0",
        "jest": "^29.7.0",
        "jsonwebtoken": "^9.0.0",
        "polished": "^4.2.2",
        "preact": "^10.19.3",
        "preact-feather": "^4.2.1",
        "prettier": "^3.2.5",
        "prettier-eslint": "^16.3.0",
        "regenerator-runtime": "^0.14.1",
        "styled-components": "6.1.8",
        "tiny-atom": "^4.2.2",
        "typescript": "^5.3.3",
        "uuid": "^8.3.2",
        "zip-a-folder": "^1.1.3"
    }
}
