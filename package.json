{
    "name": "jira-log-time",
    "version": "3.6.0",
    "description": "Code repo for [Jira Log Time](https://chrome.google.com/webstore/detail/jira-log-time/peboekgeiffcaddndeonkmkledekeegl) chrome plugin.",
    "main": "jira-api.js",
    "scripts": {
        "start": "node build/esbuild.js",
        "build": "node build/esbuild.prod.js && node build/zip-files.js",
        "minor": "node build/release.js && npm run build",
        "publish": "node build/publish.mjs",
        "patch": "node build/release.js patch && npm run build",
        "major": "node build/release.js major && npm run build",
        "lint": "eslint src cypress",
        "test": "concurrently --kill-others \"npm run test:server\" \"cypress run\"",
        "test:component": "cypress run --component",
        "test:open": "concurrently --kill-others \"npm run test:server\" \"cypress open\"",
        "cypress:open": "cypress open",
        "test:server": "node ./test-server/index.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fochlac/jira-log-time.git"
    },
    "author": "",
    "license": "GPL-3.0-or-later",
    "bugs": {
        "url": "https://github.com/fochlac/jira-log-time/issues"
    },
    "homepage": "https://github.com/fochlac/jira-log-time#readme",
    "devDependencies": {
        "@dnd-kit/core": "^6.1.0",
        "@dnd-kit/modifiers": "^7.0.0",
        "@dnd-kit/sortable": "^8.0.0",
        "@dnd-kit/utilities": "^3.2.2",
        "@sinonjs/fake-timers": "^10.0.2",
        "@types/chrome": "^0.0.263",
        "@types/firefox-webext-browser": "120.0.3",
        "@types/sinonjs__fake-timers": "^8.1.2",
        "@types/styled-components": "^5.1.23",
        "@typescript-eslint/eslint-plugin": "^7.7.0",
        "@typescript-eslint/parser": "^7.7.0",
        "chokidar": "^3.5.3",
        "chrome-webstore-upload": "^1.0.0",
        "concurrently": "^7.6.0",
        "cypress": "13.14.2",
        "cypress-devserver-esbuild": "1.4.0",
        "cypress-real-events": "^1.12.0",
        "esbuild": "0.20.0",
        "esbuild-node-externals": "^1.4.1",
        "esbuild-plugin-alias": "^0.2.1",
        "eslint": "^8.57.0",
        "eslint-config-preact": "^1.3.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-chai-friendly": "^0.7.2",
        "eslint-plugin-cypress": "2.12.1",
        "express": "^4.18.2",
        "file-saver": "^2.0.5",
        "formdata-node": "^5.0.0",
        "fs-extra": "^10.0.1",
